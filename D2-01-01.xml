<?xml version="1.0" encoding="utf-8"?>
<homegearDevice xmlns="https://homegear.eu/xmlNamespaces/HomegearDevice" version="1">
	<supportedDevices xmlns="https://homegear.eu/xmlNamespaces/DeviceType">
		<device id="D2-01-01">
			<description>Switch with local control</description>
			<typeNumber>0xD20101</typeNumber>
		</device>
	</supportedDevices>
	<properties>
		<pairingMethod>setInstallMode</pairingMethod>
	</properties>
	<functions xmlns="https://homegear.eu/xmlNamespaces/DeviceType">
		<function xmlns="https://homegear.eu/xmlNamespaces/FunctionGroupType" channel="0" type="MAINTENANCE">
			<properties>
				<internal>true</internal>
			</properties>
			<configParameters>EnOceanConfig</configParameters>
			<variables>maint_ch_values</variables>
		</function>
		<function xmlns="https://homegear.eu/xmlNamespaces/FunctionGroupType" channel="1" type="EnOceanSwitch">
			<variables>EnOceanVariables</variables>
		</function>
	</functions>
	<packets>
		<packet id="STATE_INFO">
			<direction>toCentral</direction>
			<type>0xD2</type>
			<channel>1</channel>
			<binaryPayload>
				<element>
					<bitIndex>8</bitIndex>
					<bitSize>8</bitSize>
					<constValueInteger>4</constValueInteger>
				</element>
				<element>
					<bitIndex>19</bitIndex>
					<bitSize>5</bitSize>
					<constValueInteger>0</constValueInteger>
				</element>
				<element>
					<bitIndex>25</bitIndex>
					<bitSize>7</bitSize>
					<parameterId>STATE</parameterId>
				</element>
			</binaryPayload>
		</packet>
		<packet id="STATE_SET">
			<direction>fromCentral</direction>
			<type>0xD2</type>
			<subtype>1</subtype>
			<binaryPayload>
				<element>
					<bitIndex>8</bitIndex>
					<bitSize>8</bitSize>
					<constValueInteger>1</constValueInteger>
				</element>
				<element>
					<bitIndex>24</bitIndex>
					<bitSize>8</bitSize>
					<parameterId>STATE</parameterId>
				</element>
			</binaryPayload>
		</packet>
	</packets>
	<parameterGroups xmlns="https://homegear.eu/xmlNamespaces/DeviceType">
		<configParameters id="EnOceanConfig">
			<parameter id="RESEND_TIMEOUT">
				<properties>
					<readable>true</readable>
					<writeable>true</writeable>
				</properties>
				<logicalInteger>
					<minimumValue>0</minimumValue>
					<maximumValue>10000</maximumValue>
					<defaultValue>1000</defaultValue>
				</logicalInteger>
				<physicalNone>
					<operationType>config</operationType>
				</physicalNone>
			</parameter>
			<parameter id="RESENDS_WHEN_NO_ACK">
				<properties>
					<readable>true</readable>
					<writeable>true</writeable>
				</properties>
				<logicalInteger>
					<minimumValue>0</minimumValue>
					<maximumValue>20</maximumValue>
					<defaultValue>0</defaultValue>
				</logicalInteger>
				<physicalNone>
					<operationType>config</operationType>
				</physicalNone>
			</parameter>
		</configParameters>
		<variables id="maint_ch_values">
			<parameter id="UNREACH">
				<properties>
					<readable>true</readable>
					<writeable>false</writeable>
					<service>true</service>
				</properties>
				<logicalBoolean />
				<physicalBoolean>
					<operationType>internal</operationType>
				</physicalBoolean>
			</parameter>
			<parameter id="STICKY_UNREACH">
				<properties>
					<readable>true</readable>
					<writeable>true</writeable>
					<service>true</service>
					<sticky>true</sticky>
				</properties>
				<logicalBoolean />
				<physicalBoolean>
					<operationType>internal</operationType>
				</physicalBoolean>
			</parameter>
			<parameter id="RSSI_DEVICE">
				<properties>
					<writeable>false</writeable>
				</properties>
				<logicalInteger/>
				<physicalInteger groupId="RSSI_DEVICE">
					<operationType>internal</operationType>
				</physicalInteger>
			</parameter>
			<parameter id="RF_CHANNEL">
				<properties>
					<writeable>false</writeable>
				</properties>
				<logicalInteger>
					<minimumValue>-1</minimumValue>
					<maximumValue>127</maximumValue>
					<defaultValue>-1</defaultValue>
				</logicalInteger>
				<physicalInteger groupId="RF_CHANNEL">
					<operationType>internal</operationType>
				</physicalInteger>
			</parameter>
		</variables>
		<variables id="EnOceanVariables">
			<parameter id="STATE">
				<properties>
					<readable>true</readable>
					<writeable>true</writeable>
					<casts>
						<booleanInteger>
							<trueValue>0x64</trueValue>
							<falseValue>0</falseValue>
							<threshold>1</threshold>
						</booleanInteger>
					</casts>
				</properties>
				<logicalBoolean>
					<defaultValue>false</defaultValue>
				</logicalBoolean>
				<physicalInteger groupId="STATE">
					<operationType>command</operationType>
				</physicalInteger>
				<packets>
					<packet id="STATE_INFO">
						<type>event</type>
					</packet>
					<packet id="STATE_SET">
						<type>set</type>
						<responseId>ANY</responseId>
					</packet>
				</packets>
			</parameter>
		</variables>
	</parameterGroups>
</homegearDevice>